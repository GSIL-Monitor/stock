{
    "presets": [
        [
            "latest",
            {
                " ": {
                    "modules": false
                }
            }
        ],
        "stage-2"
    ],
    "plugins": [
        "lodash",
        "transform-runtime",
        "transform-vue-jsx",
        "transform-es2015-modules-commonjs",
        [
            "component",
            [
                {
                    "libraryName": "element-ui",
                    "styleLibraryName": "theme-chalk"
                }
            ]
        ]
    ],
    "comments": false,
    "env": {
        "test": {
            "presets": ["latest", "stage-2"],
            "plugins": ["istanbul"]
        }
    }
}
