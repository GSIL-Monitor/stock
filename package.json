{
    "name": "stocks-web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "vue-cli-service build && npm run postbuild",
        "postbuild": "sw-precache --root cdn/stocks --config sw/sw-config.js",
        "dev": "vue-cli-service serve --open",
        "prereport": "vue-cli-service build --report",
        "report": "opn ./cdn/report.html"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "element-ui": "^2.4.5",
        "intersection-observer": "^0.5.0",
        "lodash": "^4.17.10",
        "number-precision": "^1.2.0",
        "vue": "^2.5.16",
        "vue-awesome-swiper": "^3.1.3",
        "vue-echarts": "^3.1.1",
        "vue-infinite-scroll": "^2.0.2",
        "vue-router": "^3.0.1",
        "vue-virtual-scroller": "^0.12.2",
        "vuex": "^3.0.1",
        "vuex-persistedstate": "^2.5.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "3.0.1",
        "@vue/cli-service": "3.0.1",
        "babel-plugin-component": "^1.1.1",
        "babel-plugin-lodash": "^3.3.4",
        "dotenv": "^6.1.0",
        "image-webpack-loader": "^4.3.1",
        "less": "^3.0.4",
        "less-loader": "^4.1.0",
        "live-server": "^1.2.0",
        "opn-cli": "^3.1.0",
        "svg-url-loader": "^2.3.2",
        "sw-precache": "^5.2.1",
        "vue-cli-plugin-element": "^1.0.0",
        "vue-template-compiler": "^2.5.16"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "Chrome >= 49"
    ]
}
